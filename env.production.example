# Environment
APP_ENV=production

# App Settings
APP_NAME=FastAPI Project
APP_VERSION=1.0.0
DEBUG=False

# Database
DATABASE_URL=postgresql+psycopg2://user:password@db:5432/minha_api

# Security
# IMPORTANTE: Use uma chave forte e única em produção!
# Gere uma chave: openssl rand -hex 32
JWT_SECRET_KEY=uma-chave-mais-punk-aqui
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60

# CORS
CORS_ORIGINS=["https://yourdomain.com"]

